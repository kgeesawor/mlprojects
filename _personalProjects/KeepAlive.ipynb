    "import requests\n",
    "import time\n",
    "import random\n",
    "\n",
    "while True:\n",
    "    try:\n",
    "        requests.get('https://www.google.com')\n",
    "        print(\"Kept alive.\")\n",
    "    except:\n",
    "        print(\"Failed to keep alive.\")\n",
    "    time.sleep(random.randint(400,600))"
